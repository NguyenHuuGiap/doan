<div class="row">
  <div class="col-md-10 col-md-offset-1">
    <hr>
    <h2><%= t ".exam_list" %></h2>
    <table class="table table-hover table-bordered">
      <tr class="success">
        <th><%= t ".created_date" %></th>
        <th><%= t ".category_name" %></th>
        <th><%= t ".status" %></th>
        <th><%= t ".duration" %></th>
        <th><%= t ".question_number" %></th>
        <th class="center"><%= t ".score" %></th>
        <th><%= t ".action" %></th>
      </tr>
      <tbody id = "body_table_exam">
        <% @tests.each do |test| %>
          <%= render "test", object: test %>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
<div class="center"></div>
